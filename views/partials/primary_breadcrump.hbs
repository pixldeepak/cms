<section class="primary_breadcrumb">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                {{#each links}}
                    {{#if this.icon}}
                        <span class="fa {{this.icon}}"></span>
                    {{/if}}
                    <a class="item" href="{{this.link}}">{{name}}</a>
                    {{#if @last}}
                    {{else}}
                        <span class="fa fa-chevron-right chevron leaves"></span>
                    {{/if}}
                {{/each}}
            </div>
        </div>
    </div>
</section>
